    "     mel_spec_file = np.loadtxt('my_features/mel_spectogram_features/mel_spectogram_' + str(idx) + '.csv', delimiter=',')\n",
    "     actual_mfcc = librosa.feature.mfcc(y=y, sr=sr)\n",
    "     actual_mel_spec = librosa.feature.melspectrogram(y=y, sr=sr)\n",
    "\n",
    "     mfcc_check = mfcc_file == actual_mfcc\n",
    "     mel_spec_check = mel_spec_file == actual_mel_spec\n",
    "\n",
    "     check.append((np.array_equal(mfcc_file, actual_mfcc),\n",
    "                   np.array_equal(mel_spec_file, actual_mel_spec)))\n",
