    "## Extracting MFCC features"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "100%|â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ| 1802/1802 [1:30:27<00:00,  3.01s/it]\n"
     ]
    }
   ],
   "source": [
    "import librosa\n",
    "import warnings\n",
    "from tqdm import tqdm\n",
    "\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "audio_path = 'deam-dataset/DEAM_audio/MEMD_audio'\n",
    "mfcc_data = []\n",
    "audio_files = sorted(os.listdir(audio_path), key=lambda f: int(f[:-4]))\n",
    "\n",
    "for filename in tqdm(audio_files):\n",
    "    if filename.endswith('mp3'):\n",
    "        y, sr = librosa.load(audio_path + '/' + filename)\n",
    "        mfcc = librosa.feature.mfcc(y=y, sr=sr)\n",
    "        mfcc_data.append(mfcc)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "os.chdir('/home/arnoldlim/CDS Project/deam-dataset/my_features')\n",
    "os.mkdir('mfcc')\n",
    "os.chdir('mfcc')\n",
    "\n",
    "idx = [i[:-4] for i in audio_files]\n",
    "for i, mfcc in zip(idx, mfcc_data):\n",
    "    np.savetxt('mfcc_' + i + '.csv', mfcc, delimiter=',')"
   ]
  },
